<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane fx:id="mainPane" focusTraversable="true" onKeyPressed="#keyPressed" prefHeight="600.0" prefWidth="700.0"
            style="-fx-background-color: &quot;0xFAF8EF&quot;;" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jobsl.cgames.sudoku.Controller">
    <children>
        <Text fx:id="mainTitle" layoutX="100.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sudoku">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Text>
        <Text fx:id="tipText" fill="#be5e56" layoutX="210.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="game running" textAlignment="CENTER" visible="false" wrappingWidth="112.0">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Text>
        <Label layoutX="348.0" layoutY="19.0" text="Level"/>
        <ChoiceBox fx:id="levelChoiceBox" layoutX="394.0" layoutY="14.0" prefHeight="30.0" prefWidth="91.0"/>
        <Button fx:id="startButton" layoutX="495.0" layoutY="14.0" mnemonicParsing="false" onAction="#startGame"
                prefHeight="30.0" prefWidth="100.0" text="New Game"/>
        <Canvas fx:id="mainCanvas" focusTraversable="true" height="495.0" layoutX="127.0" layoutY="14.0" width="495.0"
                AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0"
                AnchorPane.topAnchor="50.0"/>
    </children>
</AnchorPane>
